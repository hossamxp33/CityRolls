<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#16736a</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:fontFamily">@font/mohamed</item>
        <item name="android:textAllCaps">false</item>


    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    style="@style/Widget.AppCompat.RatingBar.Indicator"

    <style name="StarRatingBars" parent="@android:style/Widget.RatingBar">
        <item name="android:indeterminateDrawable">@color/duskYellow</item>
        <item name="android:progressDrawable">@color/lightGrey</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="colorControlNormal">@color/gray</item>
        <item name="colorControlActivated">@color/duskYellow</item>
    </style>


    <style name="MyRatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="colorAccent">@color/duskYellow</item>
        <item name="android:colorControlNormal">#f9a825</item>
    </style>
    <style name="Divider">
        <item name="android:background">?android:attr/listDivider</item> //you can give your color here. that will change all divider color in your app.
    </style>

    <style name="Divider.Horizontal" parent="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item> // You can change thickness here.

    </style>

    <style name="Divider.Vertical" parent="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>


</resources>
